{
  "compileOnSave": true,
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "module": "CommonJS",
    "allowJs": true,
    "allowUnreachableCode": false,
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "./plugins/**/*",
    "./typings/**/*",
    "./dist/**/*"
  ]
}